pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/dMichalak1/Jenkins.git'
                bat './mvnw package'
            }
        }
        stage('Report junit') {
            steps {
                junit 'target/surefire-reports/*.xml'

            }
        }
        stage('Report jacoco') {
            steps {
                jacoco()
            }
        }
         
    }
}
